    <name>idea_proof_of_work</name>
    <vendor>md11235</vendor>

    <depends>com.intellij.modules.platform</depends>

    <resource-bundle>messages.MyBundle</resource-bundle>

    <extensions defaultExtensionNs="com.intellij">
        <toolWindow factoryClass="com.github.md11235.ideaproofofwork.toolWindow.MyToolWindowFactory" id="MyToolWindow"/>
